      <Method name="saveToImageFile" group="" async="false" usecontextmenu="false" deprecated="false" unused="false" requirement="Windows NRE,macOS NRE,Android NRE" description="This method is used to print the image of a specified component as a file.">
        <Syntax text="system.saveToImageFile(objComponent, strFileName, strImageType[,strOptions [,bWholeForm]])">
